        <addForeignKeyConstraint baseColumnNames="suspended_by_id"
                                 baseTableName="suspension_history"
                                 constraintName="fk_suspension_history_suspended_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="profile"/>

    </changeSet>
</databaseChangeLog>
